
program Dryer {
	input in 0x01 0x02 8;
  	output out 0x03 0x04 16;
	
	proc Dryer {
		state Wait 
		{
			start Proc1;
		}
		state Test
		{
			
		}
	}
	proc Proc1
	{
		state Work 
		{
			start Proc2;
			start Proc3;
		}
			
			
	}
	proc Proc2
	{
		state Work2 
		{
			stop Proc1;		
		}
	}	 
	proc Proc3
	{
		state Work2 
		{
			stop;
		}
	}	
}